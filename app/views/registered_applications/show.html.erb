<h2>Applications</h2>

<div class = "row myurl">
  <div><%= @registered_application.name %></div>
  <div><%= @registered_application.url %></div>
</div>

<div class = "row edit-bookmark">
  <%= link_to "Edit Application", edit_registered_application_path, class: 'btn btn-warning' %>
</div>

<div class = "row">
  <%= link_to "Delete Application", @registered_application, method: :delete, data: {confirm: 'Are you sure you want to delete this application!?'}, class: 'btn btn-danger' %>
</div>
